{
  "name": "@trx/ui-common",
  "version": "1.0.0",
  "description": "TRX Systems - Shared UI Components and Utilities",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./themes": "./src/styles/themes.css",
    "./styles": "./src/styles/index.css",
    "./components-styles": "./dist/style.css",
    "./primevue": {
      "import": "./dist/primevue/index.js",
      "types": "./dist/primevue/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./composables": {
      "import": "./dist/composables/index.js",
      "types": "./dist/composables/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vue-tsc && vite build",
    "prepare": "husky",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "release-it",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "peerDependencies": {
    "@primevue/themes": "^4.5.0",
    "axios": "^1.7.0",
    "dayjs": "^1.11.0",
    "pinia": "^2.2.0",
    "primevue": "^4.5.0",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@primevue/themes": "^4.5.0",
    "@release-it/conventional-changelog": "^10.0.5",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "axe-core": "^4.11.1",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "primevue": "^4.5.0",
    "release-it": "^19.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "keywords": [
    "trx",
    "vue",
    "primevue",
    "components",
    "ui"
  ],
  "author": "TRIX Systems",
  "license": "Apache-2.0",
  "homepage": "https://ui.trixsystems.io",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/trixsystems/ui.git"
  },
  "bugs": {
    "url": "https://github.com/trixsystems/ui/issues"
  }
}
